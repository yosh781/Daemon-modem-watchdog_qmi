#!/bin/sh /etc/rc.common

USE_PROCD=1
START=99
DEPENDS="network"

start_service() {
    procd_open_instance
    procd_set_param command /usr/bin/modem-watchdog_qmi
    procd_set_param respawn
    procd_set_param stdout 1
    procd_set_param stderr 1
    procd_close_instance
}

stop_service() {
    killall modem-watchdog_qmi
}

restart() {
    stop
    start
}
